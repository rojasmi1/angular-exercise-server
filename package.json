  },
  "browserslist": "last 2 versions",
  "dependencies": {
    "body-parser": "1.17.1",
    "dotenv": "4.0.0",
    "@types/node": "7.0.22",
    "express": "4.15.3",
    "nodemon": "1.11.0"
  },
  "devDependencies": {
    "@types/mocha": "2.2.41",
    "@types/node": "7.0.22",
    "eslint": "4.12.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-promise": "3.5.0",
    "typescript": "2.4.0",
    "typescript-eslint-parser": "3.0.0",
    "nodemon": "1.11.0",
    "concurrently": "3.4.0"
  }
