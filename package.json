{
  "name": "o3-exercise",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:dev": "concurrently \"npm run watch\" \"npm run debug\"",
    "start:prod": "concurrently \"npm run build\" \"npm run serve\"",
    "watch": "tsc -w",
    "serve": "nodemon dist/index.js",
    "debug": "nodemon --inspect dist/index.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts' -t stylish",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' -t stylish --fix",
    "test": "tsc && mocha dist/**/*.spec.js",
    "tsc": "tsc"
  },
  "browserslist": "last 2 versions",
  "dependencies": {
    "async": "^2.1.2",
    "body-parser": "1.17.1",
    "debug": "^3.1.0",
    "dotenv": "4.0.0",
    "express": "4.15.3",
    "nodemon": "1.11.0",
    "pg": "^7.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.40",
    "@types/body-parser": "^1.16.2",
    "@types/debug": "0.0.30",
    "@types/dotenv": "^2.0.20",
    "@types/express": "^4.0.35",
    "@types/node": "7.0.22",
    "@types/pg": "^7.4.0",
    "concurrently": "3.4.0",
    "nodemon": "1.11.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}